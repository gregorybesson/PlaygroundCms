{
    "name": "playground/cms",
    "description": "Playground CMS module",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "playground",
        "playground",
        "cms"
    ],
    "homepage": "https://github.com/gregorybesson/PlaygroundCms",
    "authors": [
        {
            "name": "Greg Besson",
            "email": "gregory.besson@playground.fr",
            "homepage": "http://www.playground.fr"
        } 
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5",
		"zendframework/zendframework" : "2.*",
		"playground/core" : "2.*",
		"playground/design" : "2.*"
		
    },
	"require-dev" : {
		"phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "2.*",
        "satooshi/php-coveralls": "dev-master"
	},
    "autoload" : {
		"psr-0" : {
			"PlaygroundCms" : "src/"
		}
	},
	"extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
